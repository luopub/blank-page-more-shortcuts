{
  "manifest_version": 3,
  "name": "空白页快捷方式",
  "version": "1.0.0",
  "description": "在空白标签页显示最近浏览页面的快捷方式，支持自定义格式和数量",
  "permissions": [
    "storage",
    "tabs",
    "history"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "空白页快捷方式设置"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}