{
  "manifest_version": 3,
  "name": "Blank Page Shortcuts",
  "version": "1.0.0",
  "description": "Display shortcuts of recently visited pages on the blank tab page, supports custom format and quantity",
  "permissions": [
    "storage",
    "tabs",
    "history"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Blank Page Shortcuts Settings"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}